<launch>
	<param name="/use_sim_time" value="true"/>

	<arg name="map" default="maze"/>

	<!--  ******************** Stage ********************  -->  
	<node pkg="stage_ros" type="stageros" name="stageros" args="/home/ros/$(arg map).world">
		<param name="base_watchdog_timeout" value="0.5"/>
	</node>

	<!--  ****** Maps *****  -->
	<node name="map_server" pkg="map_server" type="map_server" args="/home/ros/$(arg map).yaml">
		<param name="frame_id" value="/map"/>
	</node>
</launch>
